{
    "name": "wasabi/zf2-skeleton-application",
    "description": " ZF2 Skeleton Application - customized by wasabilib.org",
    "license": "MIT License",
    "keywords": [
        "framework",
        "zf2",
        "wasabilib"
    ],
    "homepage": "http://framework.zend.com/",
    "require": {
        "php": ">=5.3.3",
        "zendframework/zendframework": "2.5.*",
        "zendframework/zend-form": "2.5.*",
        "wasabi/wasabilib":">=0.0.5",
        "wasabi/wasabimail":">=1.0"
    },
    "autoload": {
        "psr-0": {
            "ComposerScript\\Installer" : ""
        }
    },
    "scripts": {
        "post-install-cmd": [
            "ComposerScript\\Installer::postInstall"
        ],
        "post-update-cmd": [
            "ComposerScript\\Installer::postUpdate"
        ],
        "post-create-project-cmd": [
            "php -r \"echo 'post-create-project-cmd';\""
        ]
    }
}
